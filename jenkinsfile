pipeline{
	agent any
	stages{
		stage('GetCode'){
			steps{
				echo "get the code!"
				//tools.PrintMes("This is my lib!")
			}
		}
		stage('Test'){
			steps{
				echo "test the code."
			}
		}
		stage('deploy'){
			steps{
				echo "deploy the code to unit."
			}
		}
	}
}
